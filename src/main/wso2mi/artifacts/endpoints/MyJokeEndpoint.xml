<?xml version="1.0" encoding="UTF-8"?>
<endpoint name="MyJokeEndpoint" xmlns="http://ws.apache.org/ns/synapse">
	<http method="get" uri-template="https://official-joke-api.appspot.com/random_joke">
		<timeout> </timeout>
		<suspendOnFailure>
			<initialDuration>-1</initialDuration>
			<progressionFactor>1</progressionFactor>
		</suspendOnFailure>
		<markForSuspension>
			<retriesBeforeSuspension>0</retriesBeforeSuspension>
		</markForSuspension>
	</http>
</endpoint>
